{
  "name": "apiroom",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "ace-builds": "^1.4.11",
    "axios": "^0.19.2",
    "bootstrap": "^4.5.0",
    "console-feed": "^3.0.0",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-bootstrap": "^1.0.1",
    "react-column-resizer": "^1.1.9",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-resizable": "^1.10.1",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "swagger-ui-react": "^3.25.4",
    "wasm-loader": "^1.3.0",
    "zenroom": "^2.1.0-352d4f5"
  },
  "scripts": {
    "setup:wasm": "cp ./node_modules/zenroom/dist/module/zenroom.wasm ./public/",
    "setup:ace": "cp ./src/assets/mode-zencode.js ./node_modules/ace-builds/src-noconflict/",
    "setup:zenroom": "replace \"'wasmBinaryFile = locateFile'\" \"'//wasmBinaryFile = locateFile'\" node_modules/zenroom/dist/module/zenroom.js",
    "prestart": "npm run setup:wasm && npm run setup:ace && npm run setup:zenroom",
    "start": "react-scripts start",
    "build": "npm run setup:wasm && npm run setup:ace && npm run setup:zenroom && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "zenroom:next": "npm uninstall zenroom && npm install zenroom@next",
    "zenroom:stable": "npm uninstall zenroom && npm install zenroom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "react-app-rewired": "^2.1.6",
    "replace": "^1.2.0"
  }
}
